<script type="text/javascript">		

// 1. 배열의 함수 확인

	var arr = ['김태희','송혜교','전지현','김고은','임수정'];
	document.writeln( arr + '<br/>');//김태희,송혜교,전지현,김고은,임수정
	
	document.writeln( arr.join('/') + '<br/>'); //김태희/송혜교/전지현/김고은/임수정
	document.writeln( arr.join('or') + '<br/>'); //김태희/송혜교/전지현/김고은/임수정
	document.writeln( arr + '<br/>'); //원본 수정 확인
	
	document.writeln( arr.slice(1,3) + '<br/>'); //송혜교, 전지현
	document.writeln( arr + '<br/>'); //원본 수정 확인
	document.writeln( arr.slice(3) + '<br/>'); //김고은
	
	document.writeln( arr.reverse(3) + '<br/>'); //임수정, 김고은, 전지현, 송혜교, 김태희
	document.writeln( arr + '<br/>'); //원본 수정 확인
	
	document.writeln( arr.sort(3) + '<br/>'); //김태희, 교송혜, 전지현, 고김은, 수임정
	document.writeln( arr + '<br/>'); //원본 수정 확인
	
	
	
	// 2. 배열을 스택과 큐로 이용하기
	/* 	var data = [];
		data.push('박서준');
		data.push('현빈');
