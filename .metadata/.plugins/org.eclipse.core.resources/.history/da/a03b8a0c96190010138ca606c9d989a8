package basic;

public class EmpVO {

	private int empno; 		//사번//원래 사번, 학번, 전번, 주민번호 문자열로 잡아. 이건 80년대 만들어진 예문이라 int형
	private String ename; 	//사원명
	private int deptno; 	//부서번호
	private String job; 	//업무
	private int sal; 		//월급
	
	//생성자 함수와 setter getter를 자동으로 만들기

	public EmpVO() {
		
	}
	
	
	public EmpVO(int empno, String ename, int deptno, String job, int sal) {
		//super();
		this.empno = empno;
		this.ename = ename;
		this.deptno = deptno;
		this.job = job;
		this.sal = sal;
	}


	public int getEmpno() {
		return empno;
	}


	public void setEmpno(int empno) {
		this.empno = empno;
	}


	public String getEname() {
		return ename;
	}


	public void setEname(String ename) {
		this.ename = ename;
	}


	public int getDeptno() {
		return deptno;
	}


	public void setDeptno(int deptno) {
		this.deptno = deptno;
	}


	public String getJob() {
		return job;
	}


	public void setJob(String job) {
		this.job = job;
	}


	public int getSal() {
		return sal;
	}


	public void setSal(int sal) {
		this.sal = sal;
	}
	
	
	
	
}
