 /* 
for(let item of m){
	document.write(item+'<br/>');
}//key가 동일한 애가 들어오면 값을 덮어쓴다. 맵의 특성

for(let item of m){ //하나의 변수에
	document.write(item[0]+":"+item[1]+'<br/>');
}

for(let [k,v] of m){ //키와 값을 각각 따로
